<form action="/bankaccounts" method="post">
    <label for="bank"> Enter Bank</label>
     <input type="text" name="bank">
     <select name="user">
       <% User.all.each do |user|%>
         <option value="<%= user.id%>"><%=user.name%></option>
       <% end %>
     </select>
     <input type="submit" value="Submit">


</form>
